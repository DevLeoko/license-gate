				}
			},
			"get": {
				"operationId": "List",
				"responses": {
					"200": {
						"description": "List of licenses and total count.",
						"content": {
							"application/json": {
								"schema": {
									"properties": {
										"count": {
											"type": "number",
											"format": "double"
										},
										"licenses": {
											"items": {
												"$ref": "#/components/schemas/License"
											},
											"type": "array"
										}
									},
									"required": [
										"count",
										"licenses"
									],
									"type": "object"
								}
							}
						}
					},
					"401": {
						"description": "Unauthorized",
						"content": {
							"application/json": {
