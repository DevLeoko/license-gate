					"id": {
						"type": "integer",
						"format": "int32",
						"description": "License ID."
					},
					"active": {
						"type": "boolean",
						"description": "Whether the license is active.\r\nWhen a license is disabled, it will not validate."
					},
					"userId": {
						"type": "integer",
						"format": "int32",
						"description": "User ID that owns this license (you)."
