					"expirationDate": {
						"type": "string",
						"format": "date-time",
						"description": "Expiration date of the license.\r\nSee https://docs.licensegate.io/restriction-options/expiration",
						"default": null
					},
					"validationPoints": {
						"type": "number",
						"format": "double",
