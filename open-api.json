						"nullable": true,
						"description": "Interval to replenish validation points.\r\nSet to null for no rate limiting.\r\nSee https://docs.licensegate.io/restriction-options/rate-limit",
						"default": null
					},
					"createdAt": {
						"type": "string",
						"format": "date-time",
						"description": "Date the license was created."
					}
